# https://editorconfig.org/

root = true

[*]
end_of_line              = crlf
indent_size              = 4
indent_style             = space
insert_final_newline     = true
trim_trailing_whitespace = true

[.github/workflows/**.yml]
indent_size = 2

[*.py]
charset         = utf-8
max_line_length = 79
